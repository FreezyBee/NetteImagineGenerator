{
  "name": "freezy-bee/nette-imagine-generator",
  "license": [
    "MIT"
  ],
  "type": "library",
  "description": "Image Imagine generator for Nette Framework",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "nette",
    "imagine"
  ],
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "imagine/imagine": "^0.7",
    "freezy-bee/prepend-route": "~1.0",
    "latte/latte": "~2.4",
    "nette/di": "~2.4",
    "nette/application": "~2.4"
  },
  "require-dev": {
    "nette/bootstrap": "~2.4",
    "tracy/tracy": "~2.4",
    "nette/tester": "~2.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan-shim": "^0.9.2"
  },
  "autoload": {
    "psr-4": {
      "FreezyBee\\NetteImagineGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FreezyBee\\NetteImagineGenerator\\Tests\\": "tests/"
    }
  }
}
